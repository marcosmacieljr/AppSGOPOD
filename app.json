{
  "expo": {
    "name": "sgo",
    "slug": "sgo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": false
        }
      ]
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon_CPIA.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.transnordestina.appsgo",
      "versionCode": 1
    }
  }
}
